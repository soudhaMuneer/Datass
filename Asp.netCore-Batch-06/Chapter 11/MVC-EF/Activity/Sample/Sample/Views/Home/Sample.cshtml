@{
}
@model List<Student>;
<style>
    table,th,td{
        border:1px solid black;
    }
</style>
<table>
    <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>MARKS</th>
        <th>STATUS</th>
    </tr>
   
    @foreach(var item in Model)
        {
    <tr>
       <td>@item.Id</td>
       <td>@item.Name</td>
       <td>@item.Marks</td>
      @if(@item.Marks >= 40 )
       {
           <td>Passed</td>
       }
       else
       {
           <td>Failed</td>
       }
   </tr>
        }
</table>
