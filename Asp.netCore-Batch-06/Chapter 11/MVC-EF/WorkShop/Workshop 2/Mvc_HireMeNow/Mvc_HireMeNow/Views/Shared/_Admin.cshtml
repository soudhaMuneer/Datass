<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Page Title</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='main.css'>

    <link rel="icon" type="image/x-icon" href="favicon.ico">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/solid.min.css" rel="stylesheet" />
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/svg-with-js.min.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/chartjs-plugin-datalabels/2.2.0/chartjs-plugin-datalabels.min.js" integrity="sha512-JPcRR8yFa8mmCsfrw4TNte1ZvF1e3+1SdGMslZvmrzDYxS69J7J49vkFL8u6u8PlPJK+H3voElBtUCzaXj+6ig==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
   <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css"> 
  <style>
    
</style>
    <link href="~/adminimages/styles/adminstyle.css" rel="stylesheet" />
   <script src="~/scripts/main.js"></script>
</head>
<body>
     <div class="container-fluid">
            <div class="row flex-nowrap">
            <div class="col-auto col-md-3 col-xl-2  px-0 pt-3 " id="sidebar">
                <img class="img-fluid mx-2"  src="~/adminimages/images/Group 8.png">
                <img class="img-fluid ml-2 imgLogo" src="~/adminimages/images/Jobie.png">
                <div class="d-flex flex-column  pt- text-white min-vh-100" id="sidebar-div">
                   
                    
                    <ul class="nav nav-pills flex-column mb-sm-auto mb-0 align-items-center align-items-sm-start pt-3" id="menu">
                        <li class="nav-item py-1" id="listitems" >
                            <a href="#" class="nav-link px-0 align-middle link-light"   >
                                <i class="ms-3 fs-5 bi-house-fill"></i>
                                <span class="ms-4 d-none d-sm-inline" >Dashboard</span>
                            </a>
                        </li>
                        <li class="nav-item py-1  ">
                            <a href="#"  class="nav-link px-0 align-middle link-light"  >
                                <i class="ms-3 fs-5  bi bi-people-fill"></i>
                                <span class="ms-4 d-none d-sm-inline">Job Providers</span> </a>
                        </li>
                        <li  class="nav-item py-1">
                            <a href="#" class="nav-link px-0 align-middle link-light"  >
                                <i class="ms-3 fs-5  bi bi-file-text-fill"></i>
                                <span class="ms-4 d-none d-sm-inline">New Registrations</span></a>
                        </li>
                        <li  class="nav-item py-1">
                            <a href="~/Admin/JobSeekerListing" class="nav-link  px-0 align-middle link-light">
                                <i class="ms-3 fs-5  bi bi-search"></i>
                                <span class="ms-4 d-none d-sm-inline">Job Seekers</span>
                            </a>                   
                        </li>
                        <li  class="nav-item py-1">
                            <a href="~/Admin/JobList" class="nav-link px-0 align-middle link-light">
                                <i class="ms-3 fs-5 bi-grid-fill"></i>
                                <span class="ms-4 d-none d-sm-inline">Jobs</span></a>
                        </li>
                        <li  class="nav-item py-1">
                            <a href="~/Admin/Edit" class="nav-link px-0 align-middle link-light">
                                <i class="ms-3 fs-5 bi bi-person-fill"></i>
                                <span class="ms-4 d-none d-sm-inline">My Profile</span> </a>
                        </li>
                        <li  class="nav-item py-1">
                            <a href="#" class="nav-link px-0 align-middle link-light">
                                <i class="ms-3 fs-5 bi bi-envelope-fill"></i>
                                <span class="ms-4 d-none d-sm-inline">Messages</span> </a>
                        </li>
                        <li  class="nav-item py-1">
                            <a href="#" class="nav-link px-0 align-middle link-light">
                                <i class="ms-3 fs-5 bi bi-gear-fill"></i>
                                <span class="ms-4 d-none d-sm-inline">Settings</span> </a>
                        </li>
                        <li  class="nav-item py-1">
                            <a href="~/" class="nav-link px-0 align-middle  link-light">
                                <span class=" ms-4 ms-1 d-none d-sm-inline">Logout</span> </a>
                        </li> 
                      
                        
                        
                    </ul>
                   
                    <div class="py-3 "></div>
                </div>
            </div>
            <div style="
    flex-shrink: inherit;
">
        @RenderBody()
    </div>
      </div>
</body>
 @await RenderSectionAsync("Scripts", required: false)
</html>
